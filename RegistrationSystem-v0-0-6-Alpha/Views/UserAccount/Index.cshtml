@model  Dictionary<string, object>
@{
    ViewBag.Title = "User Account";
    ViewBag.HeaderTitle = "User Details";
}

<table class="table table-sm table-bordered m-1 p-1">
    @foreach (var kvp in Model) {
        <tr><th>@kvp.Key</th><td>@kvp.Value</td></tr>
    }
</table>

@if(User?.Identity?.IsAuthenticated ?? false)
{
    <div class="text-right">
        <a asp-controller="Home" asp-action="Index" class="btn btn-danger btn-sm text-right">Ok</a>
    </div>
}